{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-unused-vars": "off",
    "func-names": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "react/prefer-stateless-function": "off"
  }
}
